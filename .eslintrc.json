    "extends": [
        "eslint:recommended",
        "prettier",
        "prettier/react"
    ],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
